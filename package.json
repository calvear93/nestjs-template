{
  "version": "0.0.1",
  "name": "nestjs-template",
  "title": "NestJS Template",
  "description": "NestJS template",
  "author": "Alvear Candia, Cristopher Alejandro <calvear93@gmail.com>",
  "license": "MIT",
  "private": false,
  "repository": "https://github.com/calvear93/nestjs-template",
  "keywords": [
    "nest",
    "node"
  ],
  "scripts": {
    "start:debug": "env-cmd -e default,debug -r .env.js nest start --debug --watch",
    "start:development": "env-cmd -e default,development -r .env.js nest start --debug --watch",
    "start:qa": "env-cmd -e default,qa -r .env.js nest start --debug --watch",
    "build:development": "env-cmd -e default,build,development -r .env.js npm run _build",
    "build:qa": "env-cmd -e default,build,qa -r .env.js npm run _build",
    "build:production": "env-cmd -e default,build,production -r .env.js npm run _build",
    "build:start": "node dist/main",
    "orm:debug": "env-cmd -e default,build,debug -r .env.js npm run _orm --",
    "orm:development": "env-cmd -e default,build,development -r .env.js npm run _orm --",
    "orm:qa": "env-cmd -e default,build,qa -r .env.js npm run _orm --",
    "orm:production": "env-cmd -e default,build,production -r .env.js npm run _orm --",
    "test:debug": "env-cmd -e default,test,debug -r .env.js jest --watch",
    "test:development": "env-cmd -e default,test,development -r .env.js jest",
    "test:qa": "env-cmd -e default,test,qa -r .env.js jest",
    "test:production": "env-cmd -e default,test,production -r .env.js jest",
    "test:coverage": "jest --coverage",
    "test:inspect": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "lint:analyze": "eslint {src,apps,libs,test}/**/*.ts",
    "lint:fix": "eslint --fix {src,apps,libs,test}/**/*.ts",
    "format": "prettier --write src/**/*.ts test/**/*.ts",
    "_build": "rimraf dist && nest build && npx babel dist --out-dir dist",
    "_orm": "ts-node --transpile-only node_modules/typeorm/cli -f src/database/orm.config.ts",
    "_clean": "rimraf dist"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/swagger": "^4.8.0",
    "@nestjs/typeorm": "^7.1.5",
    "date-fns": "^2.21.3",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.7",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.32"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.2",
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/testing": "^7.6.15",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.0",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "env-cmd": "^10.1.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-decorator-position": "^2.2.25",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsdoc": "^34.8.2",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-radar": "^0.2.1",
    "jest": "^26.6.3",
    "prettier": "^2.3.0",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.1.2",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
