        "test:qa": "env -e qa -m test : jest",
        "test:prod": "env -e prod -m test : jest",
        "test:coverage": "env -e qa -m test : jest --collectCoverage",
        "env:schema": "env schema -e dev -m build --ci",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint --fix src/**/*.ts",
        "_build": "nest build && babel dist --out-dir dist"
