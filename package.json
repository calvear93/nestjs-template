{
  "version": "0.0.1",
  "name": "nestjs-template",
  "title": "NestJS Template",
  "description": "NestJS template",
  "author": "Alvear Candia, Cristopher Alejandro <calvear93@gmail.com>",
  "license": "MIT",
  "private": false,
  "repository": "https://github.com/calvear93/nestjs-template",
  "keywords": [
    "node",
    "nest",
    "express",
    "typeorm",
    "worker",
    "threads",
    "eslint",
    "typescript"
  ],
  "scripts": {
    "start:dev": "env-cmd -e dev,debug -r environment/loader.js nest start --debug --watch",
    "start:qa": "env-cmd -e qa,debug -r environment/loader.js nest start --debug --watch",
    "build:dev": "env-cmd -e dev,build -r environment/loader.js npm run _build",
    "build:qa": "env-cmd -e qa,build -r environment/loader.js npm run _build",
    "build:prod": "env-cmd -e prod,build -r environment/loader.js npm run _build",
    "build:start": "node dist/main",
    "orm:dev": "env-cmd -e dev,build -r environment/loader.js npm run _orm --",
    "orm:qa": "env-cmd -e qa,build -r environment/loader.js npm run _orm --",
    "orm:prod": "env-cmd -e prod,build -r environment/loader.js npm run _orm --",
    "test:dev": "env-cmd -e dev,test -r environment/loader.js jest",
    "test:qa": "env-cmd -e qa,test -r environment/loader.js jest",
    "test:prod": "env-cmd -e prod,test -r environment/loader.js jest",
    "test:coverage": "jest --coverage",
    "test:inspect": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "lint": "eslint {src,test}/**/*.ts",
    "lint:fix": "eslint --fix {src,test}/**/*.ts",
    "_build": "nest build && babel dist --out-dir dist",
    "_orm": "ts-node -r tsconfig-paths/register --transpile-only node_modules/typeorm/cli -f src/database/default/database.config.ts"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.9",
    "@nestjs/core": "^8.0.9",
    "@nestjs/platform-express": "^8.0.9",
    "@nestjs/swagger": "^5.1.0",
    "@nestjs/terminus": "^8.0.1",
    "@nestjs/typeorm": "^8.0.2",
    "luxon": "^2.0.2",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.1",
    "swagger-ui-express": "^4.1.6",
    "threads": "^1.7.0",
    "typeorm": "^0.2.37"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@nestjs/cli": "^8.1.2",
    "@nestjs/schematics": "^8.0.3",
    "@nestjs/testing": "^8.0.9",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/luxon": "^2.0.4",
    "@types/node": "^16.10.2",
    "@types/pg": "^8.6.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "babel-plugin-minify-constant-folding": "^0.5.0",
    "babel-plugin-minify-dead-code-elimination": "^0.5.1",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "env-cmd": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-decorator-position": "^4.0.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jest": "^24.5.0",
    "eslint-plugin-jest-formatting": "^3.0.0",
    "eslint-plugin-jsdoc": "^36.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-sonarjs": "^0.10.0",
    "jest": "^27.2.4",
    "nock": "^13.1.3",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">=14.17.0"
  }
}
