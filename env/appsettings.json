{
    "default": {
        "API_PREFIX": "api/<base-path>",
        "TIME_ZONE": "America/Santiago",
        "SQL": {
            "DEFAULT_DB_CONNECTION": "postgres",
            "DEFAULT_DB_PORT": 5432,
            "DEFAULT_DB_SCHEMA": "public",
            "DEFAULT_DB_ORM_SYNCHRONIZE": false
        }
    },
    "mode": {
        "build": {
            "NODE_ENV": "production",
            "PORT": 8080,
            "SQL": {
                "DEFAULT_DB_ORM_LOGGING": false,
                "DEFAULT_DB_ORM_RUN_MIGRATIONS": true
            }
        },
        "debug": {
            "NODE_ENV": "development",
            "DEBUG": "express:*,worker:*,threads:*",
            "NEST_DEBUG": true,
            "PORT": 4001,
            "SQL": {
                "DEFAULT_DB_ORM_LOGGING": true,
                "DEFAULT_DB_ORM_RUN_MIGRATIONS": false
            }
        },
        "test": {
            "NODE_ENV": "test",
            "SQL": {
                "DEFAULT_DB_ORM_LOGGING": false,
                "DEFAULT_DB_ORM_RUN_MIGRATIONS": false
            }
        }
    },
    "env": {}
}
