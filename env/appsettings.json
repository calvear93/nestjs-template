{
	"$schema": "../node_modules/@calvear/env/schemas/env.schema.json",
	"|DEFAULT|": {
		"APP_ENV": "[[ENV]]",
		"APP_NAME": "[[NAME]]",
		"APP_VERSION": "[[VERSION]]",
		"TZ": "America/Santiago",
		"LANG": "es-CL.UTF-8",
		"SWAGGER_UI": true,
		"BASE_URL": "api/(((base-path)))",
		"SECURITY": {
			"ENABLED": false,
			"HEADER_NAME": "x-api-key"
		}
	},
	"|MODE|": {
		"build": {
			"NODE_ENV": "production",
			"PORT": 8080
		},
		"debug": {
			"NODE_ENV": "development",
			"PORT": 4004,
			"BASE_URL": "api",
			"SECURITY": {
				"API_KEY": "debug"
			}
		},
		"test": {
			"NODE_ENV": "test"
		}
	},
	"|ENV|": {
		"dev": {},
		"release": {}
	},
	"|LOCAL|": {
		"dev": {},
		"release": {}
	}
}
