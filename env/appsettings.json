{
	"|DEFAULT|": {
		"TZ": "America/Santiago",
		"LANG": "es-CL.UTF-8",
		"SWAGGER_UI": true,
		"API_PREFIX": "api/(((base-path)))",
		"SECURITY": {
			"ENABLED": false,
			"HEADER_NAME": "x-api-key"
		}
	},
	"|MODE|": {
		"build": {
			"NODE_ENV": "production",
			"PORT": 8080
		},
		"debug": {
			"NODE_ENV": "development",
			"#NEST_DEBUG": "true",
			"PORT": 4004,
			"API_PREFIX": "api",
			"SECURITY": {
				"API_KEY": "debug"
			}
		},
		"test": {
			"NODE_ENV": "test"
		}
	},
	"|ENV|": {
		"dev": {},
		"release": {}
	},
	"|LOCAL|": {
		"dev": {},
		"release": {}
	}
}
